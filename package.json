{
  "name": "chatterbox",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "test": "LOGGING_ENABLED=false jest --runInBand",
    "build": "tsc",
    "start": "node dist/main.js",
    "docker:build": "docker build --build-arg GOOGLE_KEY_VALUE=$GOOGLE_KEY -t chatterbox .",
    "docker:run": "docker run -dp 5009:5009 chatterbox",
    "docker-dev": "ts-node-dev --inspect=0.0.0.0:9229 src/main.ts",
    "dev": "nodemon --watch '**/*.ts' --exec 'ts-node' src/main.ts",
    "keys": "./set-key.sh"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "4.17.13",
    "@types/jest": "26.0.24",
    "@types/node": "16.9.4",
    "@types/query-string": "^6.3.0",
    "@types/ws": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/parser": "4.31.1",
    "eslint": "7.32.0",
    "eslint-plugin-no-only-tests": "2.6.0",
    "nodemon": "^2.0.15",
    "ts-jest": "27.0.5",
    "ts-node": "10.2.1",
    "ts-node-dev": "1.1.8",
    "typescript": "^4.5.5"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.{ts,css,md}": "prettier --write"
  },
  "dependencies": {
    "axios": "^0.26.0",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "firebase-admin": "^10.0.2",
    "query-string": "^7.1.1",
    "uuid": "^8.3.2",
    "uuidv4": "^6.2.12",
    "ws": "^8.5.0"
  }
